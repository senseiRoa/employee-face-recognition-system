{
  "name": "admin-panel",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:prod": "vite build && npm run copy-to-www",
    "copy-to-www": "npm run clean-www && cp -r dist/* ../www/admin/",
    "clean-www": "rm -rf ../www/admin && mkdir -p ../www/admin"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-toastification": "^2.0.0-rc.5",
    "@vueuse/core": "^10.7.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "vite": "^5.0.8",
    "@types/node": "^20.10.0",
    "sass": "^1.69.5"
  }
}