{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI (Uvicorn)",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8081",
                "--reload"
            ],
            "jinja": true,
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}